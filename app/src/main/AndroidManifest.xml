<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mushfique.taiba">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/launchappicon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/launchappicon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.mushfique.taiba.PrivateContentActivity"></activity>
        <activity android:name="com.mushfique.taiba.ImagesActivity" />
        <activity android:name="com.mushfique.taiba.YearsActivity" />
        <activity android:name="com.mushfique.taiba.PdfWebActivity" />
        <activity
            android:name="com.mushfique.taiba.FreeMtitles"
            android:theme="@style/AppTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.mushfique.taiba.MainActivity" />
        </activity>
        <activity android:name="com.mushfique.taiba.ResultsActivity" />
        <activity
            android:name="com.mushfique.taiba.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>